import java.util.*;
import java.io.*;

public class arrayStuff {
    private int[] a;
    private Random r;
    
    public arrayStuff(int n) {
	a = int[n];
	for (int i=0;i<a.length;i++) {
	    a[i] = 75+r.nextInt(76);
	}
    }
    
    public int find(int n) {
	int i = -1;
	for(i; i<a.length; i++) {
	    if (a[i].equals(n)) {
		break;
	    }
	}
	return i;
    }

    public int maxVal() {
	ans = -1;
	for (int i=0;i<a.length;i++){
	    ans = max(a[i], ans);
	}
	return ans;
    }

}
