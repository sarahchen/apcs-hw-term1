import java.io.*;
import java.util.*;

public Sorts {
    int[] data;
    int[] copy;

    public Sorts() {
	data = new int[10];
	copy = new int[10];
    }
    public Sorts(int n) {
	data = new int[n];
	data = new int[n];
    }

    public String toString() {
	String s = "";
	for(int i=0; i<data.length;i++) {
	    s = s +" "+ data[i];
	}
	return s;
    }

    public void backup() {
	for(int i=0;i<data.length;i++) {
	    copy[i] = data[i];
	}
    }

    public void restore() {
	for(int i=0;i<copy.length;i++) {
	    data[i] = copy[i];
	}
    }

    public void isort() {
	int i,j;
	for(i=1;i<data.length;i++) {
	    int tmp = data[i];
	    for(j=i-1; j>=0 && tmp < data[j]; j--) {
		data[j+1] = data[j];
	    }
	    data[j+1] = tmp;
	}
    }
    
    public void ssort() {
	int i,j;
	int m;
	for(i=0;i<data.length;i++) {
	    m = i;
	    for(j=0;j<data.length;j++) {
		if(data[j] < data[i]) {
		    m = j;
		}
	    }
	    int replace = data[i];
	    data[i] = data[m];
	    data[m] = replace;
	}
    }
}
